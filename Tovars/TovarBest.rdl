              <ColumnExpression ColumnOwner="Prodazha" ColumnName="Kolichestvo_tovarov" GroupBy="True" />
              <ColumnExpression ColumnOwner="Dogovor" ColumnName="Id_dogovora" GroupBy="True" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Prodazha" />
              <Join Type="INNER JOIN" ColumnOwner="Tovar">
                <On>
                  <From ColumnOwner="Prodazha" ColumnName="Id_tovaraa" />
                  <To ColumnOwner="Tovar" ColumnName="Id_tovara" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Dogovor">
                <On>
                  <From ColumnOwner="Prodazha" ColumnName="Id_prodazhi" />
                  <To ColumnOwner="Dogovor" ColumnName="Id_prodazhii" />
