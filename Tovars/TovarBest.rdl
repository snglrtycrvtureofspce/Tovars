  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Dipp</DataSourceName>
        <CommandText>SELECT
  Tovar.Nazvanie_tovara
  ,Prodazha.Kolichestvo_tovarov
  ,Dogovor.Id_dogovora
FROM
  Prodazha
  INNER JOIN Tovar
    ON Prodazha.Id_tovaraa = Tovar.Id_tovara
  INNER JOIN Dogovor
    ON Prodazha.Id_prodazhi = Dogovor.Id_prodazhii
GROUP BY
  Tovar.Nazvanie_tovara
  ,Prodazha.Kolichestvo_tovarov
  ,Dogovor.Id_dogovora</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
